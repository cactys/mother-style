{
  "plugins": [
    "prettier",
    "unicorn",
    "import"
  ],
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "no-debugger": "off",
    "no-console": 0
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "jest": true,
    "es6": true,
    "browser": true,
    "node": true
  }
}